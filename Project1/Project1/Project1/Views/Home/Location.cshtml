@model IEnumerable<Project1.Domain.StoreLocation>
@{
    ViewData["Title"] = "Location";
}

<h1>Location</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Items" 
                   asp-route-id="@item.StoreLocationId">@Html.DisplayFor(modelItem => item.Location)</a>
            </td>
        </tr>
}
    </tbody>
</table>
